

module SheetsToken
    API_KEY = "{{ vault_credentials_google_sheets_api_key }}" #your google cloud console API key
    SPREADSHEET_ID = "{{ vault_credentials_google_sheets_id }}" #your google sheet ID
end

module XmppCredentials
    SRC = "{{ vault_credentials_xmpp_src }}" #your XMPP sender account
    DEST = "{{ vault_credentials_xmpp_dest }}" #your XMPP destination account
    PASSWORD = "{{ vault_credentials_xmpp_password }}" #your password for your sender account
end
